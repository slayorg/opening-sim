{
	"presets": [
		"@babel/env",
		[
			"@babel/preset-typescript", 
			{
				"isTSX": true,
				"allExtensions": true,
				"jsxPragma": "h"
			}
		],
		[
			"@babel/preset-react",
			{
				"pragma": "h"
			}
		]
	],
	"plugins": [
		"@babel/proposal-class-properties",
		"@babel/proposal-object-rest-spread"
	]
}